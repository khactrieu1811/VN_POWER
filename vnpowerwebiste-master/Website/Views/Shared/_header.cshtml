@using Business.IRepostitory
@using System.Security.Claims
@inject IDataAccessService DataAccessService
@{
    var Menus = await DataAccessService.GetAllSetting();
}
<div class="container">
    <div class="row">
        @foreach (var i in Menus){
            <div class="col-md-4">
                <div class="logo">
                    <a href="#" target="_blank"><img src="~/@i.Logo" style="margin-top:30px; width:350px;height:150px" /></a>
                </div>
            </div>
            <div class="col-md-8">
                <div class="company-info">
                    @Html.Raw(i.CompanyInfo)
                </div>
            </div>
        }
    </div>
</div>